#!/system/bin/sh

set +e

busybox kill -9 $(busybox pgrep "dnsmasq_dhcp -d \-p 0") 
busybox kill -9 $(busybox pgrep dnsmasq.CBBAK) 

echo "Run dnsmasq_dhcp" 2>/dev/null

dnsmasq_dhcp -d -p 0 --dhcp-option=252 --no-ping --dhcp-authoritative $* 2>/dev/null
pid=$!
wait $pid

echo "Run dnsmasq_dhcp failed, try dnsmasq.CBBAK" 2>/dev/null

dnsmasq.CBBAK -d -p 10053 $* 2>/dev/null
pid=$!
wait $pid
